<div class="container">
  <div class="row">
    <div class="col-md-12">
      <div class="panel panel-primary">
        <div class="panel-heading" class="text-center">Resultados de la Evaluaci√≥n</div>
        <div class="panel-body">
          <div class="row">
             <i class="fa fa-bar-chart"></i>
          </div>
          <div class="row">
            <div class="col-md-12">{{high-charts chartOptions=model.options}}</div>
          </div><br><br>
          <div class="row">
            <div class="col-md-3"></div>
              <div class="col-md-6">
                <div class="table-responsive">
                  <table class="table table-sm">
                    <thead>
                      <tr>
                        <th class="text-center">Competencia</th>
                        <th class="text-center">Nivel</th>
                      </tr>
                    </thead>
                    <tbody>
                      {{#each model.teacher.skillTests as |skill|}}
                        <tr>
                          <td class="text-center">{{skill.skillTest}}</td>
                          <td class="text-center">
                            <button type="button" class="btn btn-primary btn-sm">
                              {{skill.type}}
                            </button>
                          </td>
                        </tr>
                      {{/each}}
                    </tbody>
                  </table>
                </div>
              </div>
            </div>
        </div>
      </div>
    </div>      
  </div>
</div>