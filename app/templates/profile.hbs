<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
<div class="container">
  <div class="row">
    <div class="col-md-12">
      <div class="panel panel-primary">
        <div class="panel-heading">Resultados de la Evaluación</div>
        <br>

        <div class="text-center">
          <div class="fa fa-bar-chart" style="font-size: 48px"></div>
        </div>

        <div class="text-center font-weight-bold"> Expolora aquí tus resultados</div>
        <div class="text-center">
          <button class="btn btn-primary">
            <div class="glyphicon glyphicon-download-alt"></div> Descargar Resultados
          </button>
        </div>
        
        <div class="panel-body">
          {{profile-chart teacher=model.teacher}}

          <hr>

          {{table-results teacher=model.teacher}}
        </div>

        <div class="row">
          <div class="col-md-12 text-center">
            <span class="glyphicon glyphicon-ok" style="font-size: 46px"></span>
            <div class="font-weight-bold">Resultado de tu evaluación</div>
            <br>
            <br>
          </div>
        </div>
      </div>
    </div>      
  </div>
</div>