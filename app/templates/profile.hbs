<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
<div class="container">
  <div class="row">
    <div class="col-md-12">
      <div class="panel panel-primary">
        <div class="text-center">
          <div class="fa fa-bar-chart" style="font-size: 48px"></div>
        </div>
        <div class="text-center font-weight-bold"> Expolora aquí tus resultados</div>
        <div class="panel-heading" class="text-center">Resultados de la Evaluación</div>
        <div class="panel-body">
          <div class="row">
            <div class="col-md-12">{{high-charts chartOptions=model.options}}</div>
          </div><br><br>
          <div class="row">
            <div class="col-md-3"></div>
              <div class="col-md-6">
                <div class="table-responsive">
                  <table class="table table-sm">
                    <thead>
                      <tr>
                        <th class="text-center">Competencia</th>
                        <th class="text-center">Nivel</th>
                      </tr>
                    </thead>
                    <tbody>
                      {{#each model.teacher.skillTests as |skill|}}
                        <tr>
                          <td class="text-center">{{skill.skillTest}}</td>
                          <td class="text-center">
                            <button type="button" class="btn btn-primary btn-sm">
                              {{skill.type}}
                            </button>
                          </td>
                        </tr>
                      {{/each}}
                    </tbody>
                  </table>
                </div>
              </div>
            </div>
        </div>
        <div class="row">
          <div class="col-md-12 text-center">
            <span class="glyphicon glyphicon-ok" style="font-size: 48px"></span>
            <div class="font-weight-bold">Resultado de tu evaluación</div>
          </div>

        </div>
      </div>
    </div>      
  </div>
</div>